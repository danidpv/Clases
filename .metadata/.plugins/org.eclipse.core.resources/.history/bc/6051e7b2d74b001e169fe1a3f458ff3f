package EjerciciosMario;

public class Repaso {
	/*Arrays
	  Crear un array de 50 elementos, con valores enteros aleatorios entre -10 y +10
	  Recorrer el array y contar elementos positivos, negativos y ceros tiene
	*/
	public static void main(String[] args) {
		int[] miArray = new int[60]; //Declaramos y instanciamos el array asignandole valor
		
		for(int i =0; i<miArray.length;i++) {//Recorremos el array con un for y el .length
			miArray[i]= (int) (Math.random(21)-10);
		}
	}
}
